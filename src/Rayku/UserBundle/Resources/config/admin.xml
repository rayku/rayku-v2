<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
       <service id="sonata.admin.tag" class="Rayku\UserBundle\Admin\UserAdmin">
          <tag name="sonata.admin" manager_type="orm" group="User" label="Users"/>
          <argument />
          <argument>Rayku\UserBundle\Entity\User</argument>
          <argument>SonataAdminBundle:CRUD</argument>
          <call method="setTranslationDomain">
              <argument>RaykuUserBundle</argument>
          </call>
      </service>
   </services>
</container>