<h1>Session list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Starttime</th>
            <th>Endtime</th>
            <th>Duration</th>
            <th>Rating</th>
            <th>Rate</th>
            <th>Question</th>
            <th>Recordingid</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('session_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{% if entity.startTime %}{{ entity.startTime|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.endTime %}{{ entity.endTime|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{{ entity.duration }}</td>
            <td>{{ entity.rating }}</td>
            <td>{{ entity.rate }}</td>
            <td>{{ entity.question }}</td>
            <td>{{ entity.recordingId }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('session_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('session_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('session_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
