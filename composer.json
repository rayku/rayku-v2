{
    "name": "symfony/framework-standard-edition",
    "description": "Rayku Online Tutoring",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/advancingu/VespolinaPaymentStripeBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/rayku/NelmioApiDocBundle"
        },
        {
            "type": "package",
            "package": {
                "name": "twilson63/ngUpload",
                "type": "component",
                "version": "0.3.6",
                "dist": {
                    "url": "https://github.com/twilson63/ngUpload/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/twilson63/ngUpload.git",
                    "type": "git",
                    "reference": "0.3.6"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "ng-upload.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpunit/phpunit": "3.7.*",
        "twig/twig": "1.12.*",
        "twig/extensions": "1.0.*@dev",
        "symfony/symfony": "2.2.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.2.*",
        "symfony/monolog-bundle": "2.2.*",
        "sensio/distribution-bundle": "2.2.*",
        "sensio/framework-extra-bundle": "2.2.*",
        "sensio/generator-bundle": "2.2.*",
        "jms/security-extra-bundle": "1.4.*",
        "jms/di-extra-bundle": "1.3.*",
        "jms/serializer-bundle": "@dev",
        "jms/payment-core-bundle": "dev-master",
        "jms/payment-paypal-bundle": "dev-master",
        "doctrine/orm": ">=2.2.3,<2.4-dev@stable",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "doctrine/migrations": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "2.1.*@dev",
        "doctrine/data-fixtures": "1.0.*@dev",
        "knplabs/gaufrette": "0.1.*",
        "knplabs/knp-menu-bundle": "1.1.x-dev",
        "sonata-project/easy-extends-bundle": "2.1.*",
        "sonata-project/intl-bundle": "2.1.*",
        "sonata-project/seo-bundle": "1.*",
        "sonata-project/formatter-bundle": "2.2.*",
        "sonata-project/doctrine-extensions": "1.*",
        "sonata-project/exporter": "1.*",
        "sonata-project/notification-bundle": "2.2.*@dev",
        "sonata-project/admin-bundle": ">=2.2.2,<2.3",
        "sonata-project/block-bundle": "2.2.*",
        "sonata-project/doctrine-orm-admin-bundle": "2.2.*",
        "friendsofsymfony/user-bundle": "*",
        "friendsofsymfony/rest-bundle": "*",
        "friendsofsymfony/rest": "*",
        "friendsofsymfony/jsrouting-bundle": "*",
        "friendsofsymfony/message-bundle": "dev-master",
        "twilson63/ngUpload": "*",
        "dflydev/markdown": "1.0.2",
        "simplethings/entity-audit-bundle": "dev-master",
        "willdurand/faker-bundle": "dev-master",
        "kriswallsmith/assetic": "*",
        "stof/doctrine-extensions-bundle": "dev-master",
        "nelmio/api-doc-bundle": "dev-master",
        "guzzle/guzzle": "*",
        "jmikola/auto-login-bundle": "dev-master",
        "vespolina/payment-stripe-bundle": "dev-master",
        "components/foundation": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/behat": "2.4.*",
        "guzzle/guzzle": "3.0.*"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-2.0": "2.0.x-dev",
            "dev-2.1": "2.1.x-dev",
            "dev-master": "2.2.x-dev"
        }
    }
}
