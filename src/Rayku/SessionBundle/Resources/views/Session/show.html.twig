<h1>Session</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Starttime</th>
            <td>{{ entity.startTime|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Endtime</th>
            <td>{{ entity.endTime|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Duration</th>
            <td>{{ entity.duration }}</td>
        </tr>
        <tr>
            <th>Rating</th>
            <td>{{ entity.rating }}</td>
        </tr>
        <tr>
            <th>Rate</th>
            <td>{{ entity.rate }}</td>
        </tr>
        <tr>
            <th>Question</th>
            <td>{{ entity.question }}</td>
        </tr>
        <tr>
            <th>Recordingid</th>
            <td>{{ entity.recordingId }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('session') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('session_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
</ul>
